@article{Operto2008,
annote = {Convolution kernels for interpolation of functional data from voxel space to the cortical surface. Good resistance to registration error. 

Weights according to products of euclid and geodesic distance (which is more sophisticated than Grova 2006's method with voronoi cells, which takes a flat average within the cell). 

±3mm Euclidean distance allowed (normal to surface)
±5mm Geodesic distance (set empirically)

Generate mask once, then use for any data projection onto the mesh. 

Estimation of normal distance from surface to voxel is tricky, masking is generally not precise enough and therefore not reliable. Instead, simply providing an approximation (3mm cortical ribbon thickness) seems to work well, each node can then disregard any voxels more than 3mm away from it in the normal sense (ie, into the brain). 

Performs better than local spheres and integration along normal.

Why is this better than Voronoi? V embeds more anatomical features. as the average value is taken from a cell whose shape is constrained by local anatomy (the shape of the cortical ribbon). This implies more voxels contribute to the interpolated value. Cortical masking for this technique helps prevent signals being mixed between the cortical ribbon and WM as a whole.

BUT, the exclusivity of the Voronoi approach is questionable (the fact that kernels are not allowed to overlap). Each voxel is bound to receive signal from various nodes so therefore its intensity value should be distributed back to multiple nodes, including those from opposite sides of a sulcus. Downweight it for sure, but don't exclude it. 

So in this approach the kernels are allowed to overlap but without straying too far out of the cortical ribbon (penalised by the normal distance term).},
author = {Operto, G and Bulot, R and Anton, J.-L. and Coulon, O},
doi = {10.1016/j.neuroimage.2007.08.039},
file = {:Users/tom/Library/Application Support/Mendeley Desktop/Downloaded/Operto et al. - 2008 - Projection of fMRI data onto the cortical surface using anatomically-informed convolution kernels.pdf:pdf},
issn = {10538119},
journal = {NeuroImage},
keywords = {anatomical,cortical surface,fmri,surface-based analysis},
month = {jan},
number = {1},
pages = {127--135},
title = {{Projection of fMRI data onto the cortical surface using anatomically-informed convolution kernels}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S1053811907007586},
volume = {39},
year = {2008}
}
@article{Hutton2008,
abstract = {The thickness of the cerebral cortex can provide valuable information about normal and abnormal neuroanatomy. High resolution MRI together with powerful image processing techniques has made it possible to perform these measurements automatically over the whole brain. Here we present a method for automatically generating voxel-based cortical thickness (VBCT) maps. This technique results in maps where each voxel in the grey matter is assigned a thickness value. Sub-voxel measurements of thickness are possible using sub-sampling and interpolation of the image information. The method is applied to repeated MRI scans of a single subject from two MRI scanners to demonstrate its robustness and reproducibility. A simulated data set is used to show that small focal differences in thickness between two groups of subjects can be detected. We propose that the analysis of VBCT maps can provide results that are complementary to other anatomical analyses such as voxel-based morphometry. {\textcopyright} 2008 Elsevier Inc. All rights reserved.},
author = {Hutton, Chloe and {De Vita}, Enrico and Ashburner, John and Deichmann, Ralf and Turner, Robert},
doi = {10.1016/j.neuroimage.2008.01.027},
file = {:Users/tom/Library/Application Support/Mendeley Desktop/Downloaded/Hutton et al. - 2008 - Voxel-based cortical thickness measurements in MRI.pdf:pdf},
isbn = {1053-8119},
issn = {10538119},
journal = {NeuroImage},
number = {4},
pages = {1701--1710},
pmid = {18325790},
publisher = {Elsevier Inc.},
title = {{Voxel-based cortical thickness measurements in MRI}},
url = {http://dx.doi.org/10.1016/j.neuroimage.2008.01.027},
volume = {40},
year = {2008}
}
@article{Attias2000,
abstract = {This paper presents a novel practical framework for Bayesian model averaging and model selection in probabilistic graphical models. Our approach approximates full posterior distributions over model parameters and structures, as well as latent variables, in an analytical manner. These posteriors fall out of a free-form optimization procedure, which naturally incorporates conjugate priors. Unlike in large sample approximations, the posteriors are generally non-Gaussian and no Hessian needs to be computed. Predictive quantities are obtained analytically. The resulting algorithm generalizes the standard Expectation Maximization algorithm, and its conver-guaranteed. We demonstrate that this approach can be applied to a large class of models in several domains, including mixture models and source separation.},
author = {Attias, Hagai},
file = {:Users/tom/Downloads/NIPS-1999-a-variational-baysian-framework-for-graphical-models-Paper.pdf:pdf},
isbn = {0262194503},
issn = {10495258},
journal = {Advances in Neural Information Processing Systems},
pages = {209--215},
title = {{A variational Bayesian framework for graphical models}},
year = {2000}
}
@article{Tucholka2012,
abstract = {Being able to detect reliably functional activity in a population of subjects is crucial in human brain mapping, both for the understanding of cognitive functions in normal subjects and for the analysis of patient data. The usual approach proceeds by normalizing brain volumes to a common three-dimensional template. However, a large part of the data acquired in fMRI aims at localizing cortical activity, and methods working on the cortical surface may provide better inter-subject registration than the standard procedures that process the data in the volume. Nevertheless, few assessments of the performance of surface-based (2D) versus volume-based (3D) procedures have been shown so far, mostly because inter-subject cortical surface maps are not easily obtained. In this paper we present a systematic comparison of 2D versus 3D group-level inference procedures, by using cluster-level and voxel-level statistics assessed by permutation, in random effects (RFX) and mixed-effects analyses (MFX). We consider different schemes to perform meaningful comparisons between thresholded statistical maps in the volume and on the cortical surface. We find that surface-based multi-subject statistical analyses are generally more sensitive than their volume-based counterpart, in the sense that they detect slightly denser networks of regions when performing peak-level detection; this effect is less clear for cluster-level inference and is reduced by smoothing. Surface-based inference also increases the reliability of the activation maps. {\textcopyright} 2012 Elsevier Inc.},
author = {Tucholka, Alan and Fritsch, Virgile and Poline, Jean Baptiste and Thirion, Bertrand},
doi = {10.1016/j.neuroimage.2012.06.019},
file = {:Users/tom/Library/Application Support/Mendeley Desktop/Downloaded/Tucholka et al. - 2012 - An empirical comparison of surface-based and volume-based group studies in neuroimaging.pdf:pdf},
isbn = {1095-9572 (Electronic)$\backslash$r1053-8119 (Linking)},
issn = {10538119},
journal = {NeuroImage},
keywords = {Between-subject variability,Cluster-level tests,Cortical surface mapping,FMRI,Mixed effects analysis,Random effects analysis,Statistical inference},
number = {3},
pages = {1443--1453},
pmid = {22732555},
publisher = {Elsevier Inc.},
title = {{An empirical comparison of surface-based and volume-based group studies in neuroimaging}},
url = {http://dx.doi.org/10.1016/j.neuroimage.2012.06.019},
volume = {63},
year = {2012}
}
@article{Penny2005,
abstract = {We describe a Bayesian estimation and inference procedure for fMRI time series based on the use of General Linear Models (GLMs). Importantly, we use a spatial prior on regression coefficients which embodies our prior knowledge that evoked responses are spatially contiguous and locally homogeneous. Further, using a computationally efficient Variational Bayes framework, we are able to let the data determine the optimal amount of smoothing. We assume an arbitrary order Auto-Regressive (AR) model for the errors. Our model generalizes earlier work on voxel-wise estimation of GLM-AR models and inference in GLMs using Posterior Probability Maps (PPMs). Results are shown on simulated data and on data from an event-related fMRI experiment. {\textcopyright} 2004 Elsevier Inc. All rights reserved.},
author = {Penny, William D. and Trujillo-Barreto, Nelson J. and Friston, Karl J.},
doi = {10.1016/j.neuroimage.2004.08.034},
