doi = {10.1016/j.neuroimage.2013.05.039},
eprint = {NIHMS150003},
file = {:Users/tom/Library/Application Support/Mendeley Desktop/Downloaded/Smith et al. - 2013 - Resting-state fMRI in the Human Connectome Project.pdf:pdf},
isbn = {9108026995001},
issn = {10538119},
