number = {October 2018},
pages = {163328},
publisher = {Elsevier},
title = {{Retinal blood vessel segmentation from diabetic retinopathy images using tandem PCNN model and deep learning based SVM}},
url = {https://doi.org/10.1016/j.ijleo.2019.163328},
volume = {199},
year = {2019}
